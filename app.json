{
  "expo": {
    "name": "Void",
    "slug": "vo1d",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/Cloq(2).png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "NEVER",
      "url": null
    },
    "splash": {
      "image": "./assets/images/Cloq(1).png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vo1d.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Cloq(2).png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "navigationBar": {
        "visible": false
      },
      "package": "com.vo1d.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-camera",
      "expo-media-library",
      "expo-notifications",
      "expo-secure-store",
      "expo-navigation-bar"
    ],
    "extra": {
      "expo-navigation-bar": {},
      "eas": {
        "projectId": "a95cc12b-c59a-4a17-88b6-0f6ac639b296"
      }
    },
    "owner": "leaf-app"
  }
}
